#include <iostream>

using namespace std;

int main() {
  const int SIZE = 5; // Array size (fixed to 5 for simplicity)
  int numbers[SIZE]; // Array to store numbers

  // Get numbers from the user
  for (int i = 0; i < SIZE; ++i) {
    cout << "Enter number " << i + 1 << ": ";
    cin >> numbers[i];
  }

  // Calculate the sum
  int sum = 0;
  for (int i = 0; i < SIZE; ++i) {
    sum += numbers[i];
  }

  // Display the sum
  cout << "\nThe sum of the numbers is: " << sum << endl;

  return 0;
}

